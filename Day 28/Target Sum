class Solution {
    public int findTargetSumWays(int[] nums, int target) {
        int sum=0;
        for(int i=0;i<nums.length;i++){
            sum+=nums[i];
        }

        sumWays(nums,target,sum,0);
        
    }

    public sumWays(int[] nums,int target,int sum,int count,int start){
        for(int i=start;i<nums.length;i++){
            int ans=sum-nums[i];
            if(ans==target){
                sumWays(nums,target,sum,count+1);
            }else{
                sumWays(nums,target,sum,count+1);
            }
        }
    }
}
