class Solution {
    public void moveZeroes(int[] arr) {
        int start=0;
        for(int j=0;j<arr.length;j++){
            if(arr[j]!=0){
                arr[start]=arr[j];
                start++;
            }
        }
        for(int i=start;i<arr.length;i++){
            arr[i]=0;
        }
    }
}
