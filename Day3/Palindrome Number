class Solution {
    public boolean isPalindrome(int n) {
        int ans=0;
        int temp=n;
        while(n>0){
            int rem=n%10;
            ans=ans*10+rem;
            n=n/10;
        }
        if(ans==temp){
            return true;
        }
        return false;
        
        
    }
}
